{
  "$id": "/schemas/SubnetInfoSchema.json",
  "description": "Schema for subnet infomation",
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "subnetId": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]+$"
    },
    "subnetName": {
      "type": "string"
    },
    "isTestnet": {
      "type": "boolean"
    }
  },
  "required": ["subnetId", "subnetName", "isTestnet"]
}
